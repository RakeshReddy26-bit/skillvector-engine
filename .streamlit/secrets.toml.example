# Copy this file to secrets.toml and fill in your actual values.
# DO NOT commit secrets.toml to version control.

# Required — powers LLM skill gap analysis
ANTHROPIC_API_KEY = "sk-ant-your-key-here"

# Optional — enables RAG job retrieval
PINECONE_API_KEY = "your-pinecone-key"
PINECONE_INDEX_NAME = "skillvector-jobs"
PINECONE_ENVIRONMENT = "us-east-1"

# Optional — enables skill prerequisite graph
NEO4J_URI = "bolt://localhost:7687"
NEO4J_USER = "neo4j"
NEO4J_PASSWORD = "password"

# Optional — admin dashboard access
ADMIN_EMAIL = "admin@example.com"
